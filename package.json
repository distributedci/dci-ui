{
  "name": "dci-app",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "repository": "https://github.com/redhat-cip/dci-ui",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rm -rf static test/e2e/reports",
    "build": "npm run clean && gulp build:dev",
    "dev": "gulp serve:dev",
    "test": "npm run build && npm run test:unit && gulp lint && npm run test:e2e",
    "test:unit": "karma start test/unit/karma.conf.js",
    "test:e2e": "nightwatch --config test/e2e/nightwatch.conf.js"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "angular-cookies": "1.6.4",
    "angular-moment": "1.0.1",
    "angular-sanitize": "1.6.4",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-router": "0.4.2",
    "bootstrap-sass": "3.3.7",
    "d3": "^4.9.1",
    "jsonformatter": "^0.6.0",
    "lodash": "4.13.1",
    "moment": "2.18.1",
    "rcue": "4.0.0-rc.1",
    "ui-select": "0.19.8"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "browserify": "^14.4.0",
    "chromedriver": "^2.30.1",
    "connect": "^3.6.2",
    "eslint": "^4.0.0",
    "globby": "^6.1.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.4.1",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "husky": "^0.13.4",
    "jasmine": "^2.6.0",
    "jasmine-reporters": "^2.2.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "lint-staged": "^3.6.0",
    "nightwatch": "^0.9.16",
    "prettier": "^1.4.2",
    "protractor": "^5.1.2",
    "serve-static": "^1.12.3",
    "through2": "^2.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
